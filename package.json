{
	"name": "namedays",
	"version": "0.0.0-development",
	"description": "A minimal namedays API",
	"scripts": {
		"build": "tsup src/index.ts --format esm,cjs --dts",
		"lint": "eslint .",
		"format": "prettier --write .",
		"format-check": "prettier --check .",
		"release": "pnpm build && pnpm changeset publish",
		"review": "pnpm install --frozen-lockfile && pnpm format-check && pnpm lint && pnpm test && pnpm build",
		"test": "vitest --run"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.1",
		"@tammergard/eslint-config-base": "3.2.0",
		"@tammergard/prettier-config": "2.0.0",
		"@tammergard/tsconfig": "2.2.0",
		"eslint": "8.37.0",
		"prettier": "2.8.7",
		"tsup": "6.7.0",
		"typescript": "5.0.2",
		"vitest": "0.29.8"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/filiptammergard/namedays.git"
	},
	"keywords": [
		"nameday",
		"namedays",
		"name day"
	],
	"author": "Filip Tammerg√•rd <filip+npm@tammergard.se> (https://tammergard.se/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/filiptammergard/namedays/issues"
	},
	"homepage": "https://github.com/filiptammergard/namedays#readme"
}
