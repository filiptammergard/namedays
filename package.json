{
  "name": "namedays",
  "version": "0.0.0-development",
  "description": "A minimal namedays API",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "review": "yarn install --frozen-lockfile && yarn format-check && yarn lint && yarn test && yarn build",
    "test": "vitest --run"
  },
  "devDependencies": {
    "@tammergard/eslint-config-base": "3.0.0",
    "@tammergard/prettier-config": "1.5.0",
    "@tammergard/semantic-release-config-npm": "1.1.0",
    "@tammergard/tsconfig": "1.0.0",
    "eslint": "8.23.1",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "tsup": "6.2.3",
    "typescript": "4.8.4",
    "vitest": "0.23.4"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filiptammergard/namedays.git"
  },
  "keywords": [
    "nameday",
    "namedays",
    "name day"
  ],
  "author": "Filip Tammerg√•rd <filip+npm@tammergard.se> (https://tammergard.se/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filiptammergard/namedays/issues"
  },
  "homepage": "https://github.com/filiptammergard/namedays#readme"
}
